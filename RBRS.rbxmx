	if core.getSecurityLevel() < config.RankToBypass then
		core.addF3XAttachment("Core", "OpenExplorer", "Intercept", function()
			return nil
		end)
		
		core.attachNewF3X(function(tool, dataTab)
			local core = dataTab["Core"]["Data"]
			while core.UI == nil do
				wait()
